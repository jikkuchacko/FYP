<script type="text/javascript">
    $(document).ready(function () {
        $("#btnImport").click(function (e) {
            e.preventDefault();
            var formData = new FormData($('form')[0]);
            setTimeout(function () {
                $.ajax({
                    url: "/ExcelUpload/Index",
                    type: 'POST',
                    contentType: false,
                    processData: false,
                    data: formData,
                    async: false,
                    success: function (response) {
                        $("#upload").val("");
                        if (response.result) {
                            debugger;
                        }
                        else {
                            debugger;
                        }
                    },
                    error: function (err) {
                        debugger;
                    }
                });
            });
        });
    });
</script>
<h2>Excel upload</h2>
<form enctype="multipart/form-data">
